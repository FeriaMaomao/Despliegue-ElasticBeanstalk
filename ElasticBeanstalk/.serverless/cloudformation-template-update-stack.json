{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "Application": {
      "Type": "AWS::ElasticBeanstalk::Application",
      "Properties": {
        "ApplicationName": "elasticbeanstalk-grafana-dev",
        "Description": "AWS Elastic Beanstalk Docker Sample Application"
      }
    },
    "ApplicationVersion": {
      "Type": "AWS::ElasticBeanstalk::ApplicationVersion",
      "Properties": {
        "ApplicationName": {
          "Ref": "Application"
        },
        "Description": "Version 1.0",
        "SourceBundle": {
          "S3Bucket": "s3-bucket-eb-dev",
          "S3Key": "Dockerrun.aws.json"
        }
      }
    },
    "ApplicationEnv": {
      "Type": "AWS::ElasticBeanstalk::Environment",
      "Properties": {
        "ApplicationName": {
          "Ref": "Application"
        },
        "Description": "AWS Elastic Beanstalk Environment running Docker",
        "SolutionStackName": "64bit Amazon Linux 2 v3.2.5 running Docker",
        "VersionLabel": {
          "Ref": "ApplicationVersion"
        },
        "OptionSettings": [
          {
            "Namespace": "aws:autoscaling:launchconfiguration",
            "OptionName": "IamInstanceProfile",
            "Value": "aws-elasticbeanstalk-ec2-role"
          }
        ]
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": "tv-s3-bucket"
    }
  }
}